public String withoutX(String str) {
  int len = str.length();
 if(str.length()>=2){
 if(str.charAt(0)=='x' && str.charAt(len-1)=='x')
    return str.substring(1,len-1);
  else if(str.charAt(0)=='x')
    return str.substring(1,len);
  else if( str.charAt(len-1)=='x')
    return str.substring(0,len-1);
 }
 else 
  return "";
  return str;
}
