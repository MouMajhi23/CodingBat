public String front3(String str) {
  String temp = "";
  if(str.length()<3)
    return str+str+str;
  else
  for(int i = 0 ; i < 3 ; i++){
    temp = temp + str.substring(0,3);
  }
  return temp;
}
or 
public String front3(String str) {
  return (str.length()>=3?str.substring(0,3)+str.substring(0,3)+str.substring(0,3):str+str+str);
}
