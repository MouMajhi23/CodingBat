public String stringYak(String str) {
   String res = "";
        for(int i=0;i<str.length()-2;i++){
           
             int end = i+3;
             if(end>str.length()){
              end = str.length();
                
             }
            
            if(str.substring(i,end).equals("yak"))
                {
                    
                    res = str.replace("yak","");
                    
                 }
        }
  return res;
              
}
